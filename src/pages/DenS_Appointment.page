	             	 		<td>
	             	 			<apex:selectList styleClass="inputOrSelList width100 " value="{!appointmentView.Client__c}" size="1"
				                				 multiselect="false"  onchange="rerenderAppointstable();">
				                     <apex:selectOptions value="{!clients}"/>
				                </apex:selectList>
