<apex:page standardController="DenS_Appointment__c"
    extensions="DenS_AppointmentController" standardStylesheets="true"
    showHeader="true" sidebar="true" recordSetVar="allappoints" id="apage">
    <!-- TODO
      -->
    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" />
    <head>
<style>
/*general*/
form {
    text-align: -webkit-center;
}
body {
    text-align: center;
    margin: auto;
    font-size: 100%;
}
.mainDiv {
    width: 800px !important;
	clear: both;
}
h1, h2, h3, h4, h5, h6 {
    font-family: Futura, Cambria, Pier, Constantia;
    color: black;
}
h4 {
    font-size: 19px;
}
h5 {
    font-size: 16px;
    font-weight: 500;
    display: block;
    font-family: Arial, Futura, Cambria, Pier, Constantia;
}
.head {
    text-align: right;
}
.left_col {
    float: left;
    width: 400px;
}
.right_col {
	width: 400px;
	float:right;
}
.form-table{
	clear: both;
	width: 100%;
}
.form-table tr {
	padding-bottom: 55px;
	border-spacing: 30px;
}
.form-table tr th, table.width100 tr th {
	width: 157px;
	text-align: right;
	padding-right: 20px;
}
.form-table tr td{
	text-align: left;
	margin-left: 10px;
}
.form-group {
	margin-bottom: 55px;
}
.form-control {
    width: 95.56%; //84.4%;//675px;
    height: 22px;
    padding: 10px 10px;
    font-size: 16px;
    line-height: 1.42857143;
}
.bold {
	font-weight: bold;
}
.inputOrSelList {
    font-size: 18px;
}
.width100 {
    width: 100%;
}
.width50 {
    width: 50%;
}
.width230px {
	width: 230px;
}
.importantClick2 {
    color: rgb(0,0,140);
    cursor: pointer;
}
.inline {
    display: inline;
}
/*calendar*/
ul {
    list-style-type: none;
}
.month ul {
    margin: 0;
    padding: 5px 0px;
    background: #1abc9c;
    color: white;
    font-size: 24px;
    text-transform: uppercase;
    letter-spacing: 3px;
    text-align: center;
}
.month .prev {
    float: left;
    padding-top: 10px;
    margin-left: 3px;
    cursor: pointer;
}
.month .next {
    float: right;
    padding-top: 10px;
    cursor: pointer;
}
.weekdayDiv {
	display: inline-flex;
	width: 24px;
}
ul.weekdays {
	text-align: justify;
    margin: 0;
    padding: 10px 20px 10px 20px;
    background-color: #ddd;
    color: #667;
    font-size: 18px;
}
ul.weekdays:after, .justify:after {
    content: "";
    display: inline-block;
    width: 100%;
}
.justify {
	text-align: justify;
	padding: 0px 2.5px 0.5px 7px;
}
.daysDiv {
    background: #eee;
    border: 3px solid black;
    padding: 5px 4px 0px 5px;
}
.daysBlock {
    font-size: 20px;
    text-align: left;
}
.dayCont {
	display: inline-flex;
    padding: 0px 0px 5px 0px;
}
.day {
    text-decoration: none;
    width: 30px;
    text-align: center;
    cursor: default;
    font-weight: 700;
    padding: 6px 10.1px;
    color: black;
}
.day1{
    background-color: rgb(0, 250, 0) !important;
    cursor: pointer;
}
.day2{
    background-color: rgb(100, 0, 0);
    opacity: 0.6;
}
.daysunday {
	color: rgb(250, 10, 10);
}
.today {
    border-bottom: solid red;
}
body a:hover {
	text-decoration: none;
}
body a.daysunday:not(.day1):hover {
	color: rgb(250, 10, 10);
}
body a:not(.day1):not(.daysunday):hover {
    color: black;
}
/*message*/
.messageTable {
    font-size: 16px;
    margin: auto !important;
}
.message {
    margin: auto;
    width: 700px;
    background-color: #8fffe9 !important;
}
.msgIcon {
    display: none;
}
/*buttons and tables*/
body .btn {
	color: black;
}
.specPadding {
    padding-right: 25px;
}
.marg80 {
    margin-left: 80px;
}
.marg30 {
    margin-left: 30px;
}
.description {
    height: 160px;
    width: 625px;
}
/*tables*/
.outer td {
    vertical-align: middle;
}
table.listTable, th, td {
    border-collapse: collapse;
    text-align: center;
}
.listTable td.leftAlign {
    padding: 0px 0px 0px 18px;
    text-align: left;
}
table.listTable th {
    padding: 0px 5px;
    border: 1px solid #1abc9c;
    border-bottom: 1px solid black;
    background: #1abc9c;
    color: white;
}
table tbody {
    border: 1px solid black;
}
.listTable tr { 
	 height: 38px;
}
.listTable tr:nth-child(odd) td {   
     background: #aeffd9;
}
.listTable tr:nth-child(even) td {  
     background: #8fffe9;
}
table.listTable td {
	padding: 0px 5px;
}
.checkbox-table th {
	font-weight: normal;
	text-align: left;
	width: 200px;
}
</style>
    </head>
    <script>
      j$ = jQuery.noConflict();
      j$(document).ready(function() {
         /*j$('[class*=configDetails').hide();    SHOULDBE
         	todentist:
        if ( "{!userProfile}" == 'Dentist') {
            j$('[id*=pageC').hide();
        }
        else if ( "{!userProfile}" == 'Client' ) { 
            j$('[id*=pageC').hide();
            j$('[id*=pageAL').hide();
        }
        */
        j$('[id*=apage\\:forma\\:DateAL').parent().hide();
        for ( j=0 ; j<6 ; j++) {
        	var1 = '[id*=AL' + j ;
        	j$(var1).hide();
        }
      });
      function toggleSearch (i) {
       	if (i==0) {
       		j$('[id*=apage\\:forma\\:DateAL').parent().toggle();
      	}
      	varrr = '[id*=AL' + i;
      	j$(varrr).toggle();
      }
      j$('[id*=appointmentDetails').ready( function(){
      	j$('[id*=appointmentDetails').hide();
      });
      function toggleDescription() {
            j$('[id*=descr').toggle();
      }
      function clickableMouseenter () {
        j$('[class*=i').css({"color": "rgb(0,0,190)"});
      }
      function clickableMouseleave () {
        j$('[class*=clickable').css({"color": "rgb(0,0,100)"});
      }
      j$('[id*=pageC').ready( function(){
            clickWeekdayC();
      });
      function clickWeekdayC() {
        callChangeConfigTypeW();
        j$('[id*=selectweekday').show();
        j$('[id*=selectdate').hide();
        j$('[class*=dateFormat').hide();
        j$('[id*=chooseWConfig').css({"border-style": "solid", "border-color": "rgb(0,0,255)/*rgb(0,185,0)*/", "border-width": "0px 0px 3px 0px", "border-radius": "5px"});
        j$('[id*=chooseSConfig').css({"border-style": "none"});
      }
      function clickSpecificC() {
        callChangeConfigTypeS();
        j$('[id*=selectweekday').hide();
        j$('[id*=selectdate').show();
        j$('[class*=dateFormat').show();
        j$('[id*=chooseSConfig').css({"border": "3px solid rgb(0,0,255) /*rgb(255,255,155)*/", "border-width": "0px 0px 3px 0px", "border-radius": "5px"});
        j$('[id*=chooseWConfig').css({"border-style": "none"});
      }
   </script>
    <apex:form id="forma">
        <apex:outputPanel id="pageA" > 
       		<!-- rendered="{!isClient}" -->
        	<p/>
	        <br/>
            <div id="aDatePick" class="mainDiv">
